<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                              http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <localRepository>${user.home}/.m2/repository</localRepository>
  <interactiveMode>true</interactiveMode>
  <offline>false</offline>
  <pluginGroups>
    <pluginGroup>org.mortbay.jetty</pluginGroup>
  </pluginGroups>
  <servers>
    <!-- Configuration des serveurs si nécessaire -->
  </servers>
  <mirrors>
    <!-- Configuration des miroirs si nécessaire -->
  </mirrors>
  <proxies>
    <!-- Configuration des proxys si nécessaire -->
  </proxies>
  <profiles>
    <!-- Configuration des profils si nécessaire -->
  </profiles>
  <activeProfiles>
    <!-- Configuration des profils actifs si nécessaire -->
  </activeProfiles>
</settings>
